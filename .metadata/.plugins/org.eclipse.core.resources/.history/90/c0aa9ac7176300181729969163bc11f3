package presentation;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

public class Lanceur {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Comment ajouter des elements à une Set et comment on ajoute des éléments au SET
		
				// declarer et initialiser la collection
				Set hash = new HashSet();
				//hash.add(12);
				hash.add("toto");
				hash.add(12.5);
				//hash.add(12);
				hash.add("toto");

				//parcours du set avec un foreach maintenant for avec Java 10
				
				for (Object item : hash) {
					
					System.out.println(item);
					//item.notifyAll();

				}
				
				// parcours avec un iterator
				
				Iterator it=hash.iterator();
				while(it.hasNext()) {
					System.out.println(it.next());
				}

	}

}
